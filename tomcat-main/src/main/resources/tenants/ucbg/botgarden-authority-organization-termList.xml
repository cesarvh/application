<record id="preferredOA">
	<section id="termInformation">
		<!-- List containing this item's preferred term and any non-preferred terms -->
		<repeat id="orgTermGroupList/orgTermGroup">
			<field id="termType">
				<options>
					<option id="">Please select a value</option>
					<option id="kewabbr">kewabbr</option>
					<option id="abbr">abbr</option>
					<option id="pref">pref</option>
					<option id="alternate descriptor">alternate descriptor</option>
				</options>
			</field>
			<!-- Remove autocomplete -->
			<field id="termSource" autocomplete=""/>
		</repeat>
	</section>
</record>
