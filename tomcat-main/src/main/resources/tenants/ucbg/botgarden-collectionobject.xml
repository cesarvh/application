				<option id="Many">Many</option>
			</options>
		</field>
		<field id="flowersApr" section="botgarden">
			<options>
				<option id="No" default="yes">No</option>
				<option id="Some">Some</option>
				<option id="Many">Many</option>
			</options>
		</field>
		<field id="flowersMay" section="botgarden">
			<options>
				<option id="No" default="yes">No</option>
				<option id="Some">Some</option>
				<option id="Many">Many</option>
			</options>
		</field>
		<field id="flowersJun" section="botgarden">
			<options>
				<option id="No" default="yes">No</option>
				<option id="Some">Some</option>
				<option id="Many">Many</option>
			</options>
		</field>
		<field id="flowersJul" section="botgarden">
			<options>
				<option id="No" default="yes">No</option>
				<option id="Some">Some</option>
				<option id="Many">Many</option>
			</options>
		</field>
		<field id="flowersAug" section="botgarden">
			<options>
				<option id="No" default="yes">No</option>
				<option id="Some">Some</option>
				<option id="Many">Many</option>
			</options>
		</field>
		<field id="flowersSep" section="botgarden">
			<options>
				<option id="No" default="yes">No</option>
				<option id="Some">Some</option>
				<option id="Many">Many</option>
			</options>
		</field>
		<field id="flowersOct" section="botgarden">
			<options>
				<option id="No" default="yes">No</option>
				<option id="Some">Some</option>
				<option id="Many">Many</option>
			</options>
		</field>
		<field id="flowersNov" section="botgarden">
			<options>
				<option id="No" default="yes">No</option>
				<option id="Some">Some</option>
				<option id="Many">Many</option>
			</options>
		</field>
		<field id="flowersDec" section="botgarden">
			<options>
				<option id="No" default="yes">No</option>
				<option id="Some">Some</option>
				<option id="Many">Many</option>
			</options>
		</field>
		<field id="fruitsJan" datatype="boolean" section="botgarden"/>
		<field id="fruitsFeb" datatype="boolean" section="botgarden"/>
		<field id="fruitsMar" datatype="boolean" section="botgarden"/>
		<field id="fruitsApr" datatype="boolean" section="botgarden"/>
		<field id="fruitsMay" datatype="boolean" section="botgarden"/>
		<field id="fruitsJun" datatype="boolean" section="botgarden"/>
		<field id="fruitsJul" datatype="boolean" section="botgarden"/>
		<field id="fruitsAug" datatype="boolean" section="botgarden"/>
		<field id="fruitsSep" datatype="boolean" section="botgarden"/>
		<field id="fruitsOct" datatype="boolean" section="botgarden"/>
		<field id="fruitsNov" datatype="boolean" section="botgarden"/>
		<field id="fruitsDec" datatype="boolean" section="botgarden"/>
	</section>

	<section id="objectHistoryAssociationInformation">
		<section id="associationInformation">
			<repeat id="assocConceptGroupList/assocConceptGroup">
				<!-- Remove autocomplete -->
				<field id="assocConcept" autocomplete=""/>
			</repeat>
		</section>
	</section>

	<section id="referenceInformation">
		<repeat id="referenceGroupList/referenceGroup">
			<!-- Remove autocomplete -->
			<field id="reference" autocomplete=""/>
		</repeat>
	</section>

	<section id="objectCollectionInformation">
		<repeat id="fieldCollectors">
			<!-- Add ui-search, and change autocomplete to organization-collector -->
			<field id="fieldCollector" autocomplete="organization-collector" ui-search="repeatable"/>
		</repeat>
		<!-- Change autocomplete -->
		<field id="fieldCollectionNumberAssignor" autocomplete="person-person,organization-collector"/>
	</section>

	<section id="accessionUseInformation">
		<repeat id="accessionUseGroupList/accessionUseGroup">
			<!-- Add autocomplete -->
			<field id="useType" ui-type="enum" autocomplete="vocab-AcessUseType"/>
			<field id="usedIn" autocomplete="concept-class_ca,concept-research_ca" ui-search="repeatable"/>
		</repeat>
	</section>
</record>