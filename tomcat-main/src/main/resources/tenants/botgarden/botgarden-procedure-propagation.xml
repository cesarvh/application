<record id="propagation" is-extension="true">
	<!--<services-url>propagations</services-url>
	<services-tenant-plural>Propagations</services-tenant-plural>
	<services-tenant-singular>Propagation</services-tenant-singular>
	<services-list-path>abstract-common-list/list-item</services-list-path>
	<services-record-path>propagations_common:http://collectionspace.org/services/propagation,propagations_common</services-record-path>-->
	<services-record-path id="collectionspace_core">collectionspace_core:http://collectionspace.org/collectionspace_core/,collectionspace_core</services-record-path>
	<!--<hassoftdelete>true</hassoftdelete>-->

    <section id="propagationInformation">
        <repeat id="scarStratGroupList/scarStratGroup">
			<field id="scarStrat" ui-type="enum" autocomplete="vocab-scarstratification">
            <!--Converted to Dynamic-->
			</field>
			<field id="duration" ui-type="validated" datatype="integer"/>
			<field id="durationUnit">
				<options>
					<option id="" default="yes">Please select a value</option>
					<option id="hours">Hours</option>
					<option id="days">Days</option>
					<option id="weeks">Weeks</option>
					<option id="months">Months</option>
				</options>
			</field>
		</repeat>
        <field id="propType" ui-search="repeatable" mini="summary,search,list,terms,relate" ui-type="enum" autocomplete="vocab-propagationType">
        <!--Converted to Dynamic-->
		</field>
		<field id="cuttingType" ui-type="enum" autocomplete="vocab-cutType">
        <!--Converted to dynamic-->
		</field>
    </section>
</record>