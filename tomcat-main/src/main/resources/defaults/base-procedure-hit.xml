	<services-tenant-singular>Hit</services-tenant-singular>
	<services-tenant-plural>Hits</services-tenant-plural>
	<web-url>hit</web-url>
	<tab-url>hit-tab</tab-url>

	<services-list-path>abstract-common-list/list-item</services-list-path>
	<services-record-path>hits_common:http://collectionspace.org/services/hit,hits_common</services-record-path>
